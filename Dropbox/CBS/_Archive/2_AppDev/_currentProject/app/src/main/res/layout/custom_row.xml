<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="false"
    android:clickable="false">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Timestamp"
        android:id="@+id/txt_starts"
        android:layout_margin="5dp"
        android:paddingStart="10dp"
        android:paddingTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Duration"
        android:id="@+id/textView"
        android:layout_below="@+id/txt_starts"
        android:layout_alignStart="@+id/txt_starts"
        android:paddingStart="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Participants"
        android:id="@+id/textView2"
        android:layout_alignTop="@+id/txt_currentpartic"
        android:layout_alignStart="@+id/textView"
        android:paddingStart="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Max Part."
        android:id="@+id/textView3"
        android:layout_below="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:paddingStart="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Training ID"
        android:id="@+id/textView4"
        android:paddingStart="10dp"
        android:layout_below="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Type"
        android:id="@+id/textView6"
        android:paddingStart="10dp"
        android:layout_alignTop="@+id/txt_teamID"
        android:layout_alignStart="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="N/A"
        android:id="@+id/txt_trainingID"
        android:layout_below="@+id/txt_maxpartic"
        android:layout_toRightOf="@+id/txt_starts" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="N/A"
        android:id="@+id/txt_duration"
        android:layout_alignTop="@+id/textView"
        android:layout_toEndOf="@+id/txt_starts" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="N/A"
        android:id="@+id/txt_maxpartic"
        android:layout_below="@+id/txt_currentpartic"
        android:layout_toRightOf="@+id/txt_starts" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="N/A"
        android:id="@+id/txt_teamID"
        android:layout_toRightOf="@+id/txt_starts"
        android:layout_below="@+id/txt_trainingID" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="N/A"
        android:id="@+id/txt_currentpartic"
        android:layout_below="@+id/txt_duration"
        android:layout_toRightOf="@+id/txt_starts" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Status"
        android:id="@+id/textView7"
        android:paddingStart="10dp"
        android:layout_below="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:paddingBottom="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="N/A"
        android:id="@+id/txt_status"
        android:layout_alignTop="@+id/textView7"
        android:layout_toEndOf="@+id/txt_starts" />

</RelativeLayout>